{
  "request": {
    "method": "POST",
    "urlPath": "/demo",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "equalToJson": {
          "name": "Marry",
          "age": 19
        }
      }
    ]
  },
  "response": {
    "status": 200,
    "body": "Hello World!"
  }
}
